{
	"name": "prosperlink-component-library",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"packages": "yarn add dayjs && yarn add -D vitest jsdom less tailwindcss @tailwindcss/forms postcss autoprefixer @testing-library/dom @testing-library/svelte"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@tailwindcss/forms": "^0.5.3",
		"@testing-library/dom": "^8.17.1",
		"@testing-library/svelte": "^3.2.1",
		"autoprefixer": "^10.4.10",
		"jsdom": "^20.0.0",
		"less": "^4.1.3",
		"postcss": "^8.4.16",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.0",
		"vitest": "^0.23.2"
	},
	"type": "module",
	"dependencies": {
		"dayjs": "^1.11.5"
	}
}
